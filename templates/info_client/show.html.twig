{% extends 'base.html.twig' %}

{% block title %}InfoClient{% endblock %}

{% block body %}
    <h1>InfoClient</h1>

    <table class="table">
        <thead>
        <th>Nom</th>
        <th>Prenom</th>
        <th>Mail_pro</th>
        <th>Nom_societe</th>
        <th>Adresse</th>
        <th>Num_pro</th>
        <th>Num</th>
        <th>Cp</th>
        <th>Ville</th>
        <th>Siret</th>
        <th></th>
        <th></th>
        </thead>
        <tbody>
        {% for info_client in clients %}
        <tr>

            <td>{{ info_client.nom }}</td>
            <td>{{ info_client.prenom }}</td>
            <td>{{ info_client.mailPro }}</td>
            <td>{{ info_client.nomSociete }}</td>
            <td>{{ info_client.adresse }}</td>
            <td>{{ info_client.numPro }}</td>
            <td>{{ info_client.num }}</td>
            <td>{{ info_client.cp }}</td>
            <td>{{ info_client.ville }}</td>
            <td>{{ info_client.siret }}</td>
            <td><a href="{{ path('app_info_client_edit', {'id': info_client.id}) }}">edit</a></td>
            <td>{{ include('info_client/_delete_form.html.twig') }}</td>

        </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_user_index') }}">back to list</a>


{% endblock %}
